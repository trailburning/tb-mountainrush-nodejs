        {{ if (game.name) { }}
          {{= game.name }}
        {{ } else { }}
          {{= game.level_name }}
        {{ } }}
        </h1>for {{= game.campaign_name }}
        </div>
        <div class="summit-container">
          <div class="summit">
            <h1>{{= game.ascent }}m</h1>{{= game.level_name }}
